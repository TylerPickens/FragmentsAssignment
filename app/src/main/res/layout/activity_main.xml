<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/temp_fragment"
        android:name="com.ebookfrenzy.assignment4fragments.TempFragment"
        android:layout_width="401dp"
        android:layout_height="254dp"
        android:layout_marginStart="169dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="169dp"
        android:layout_marginBottom="665dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.211"
        tools:layout="@layout/temp_fragment" />

    <fragment
        android:id="@+id/length_fragment"
        android:name="com.ebookfrenzy.assignment4fragments.LengthFragment"
        android:layout_width="409dp"
        android:layout_height="264dp"
        android:layout_marginStart="169dp"
        android:layout_marginEnd="169dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout="@layout/length_fragment" />

    <fragment
        android:id="@+id/mass_fragment"
        android:name="com.ebookfrenzy.assignment4fragments.MassFragment"
        android:layout_width="408dp"
        android:layout_height="217dp"
        app:layout_constraintBottom_toBottomOf="@+id/length_fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/temp_fragment"
        app:layout_constraintVertical_bias="0.0"
        tools:layout="@layout/mass_fragment" />
</androidx.constraintlayout.widget.ConstraintLayout>